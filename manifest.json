{
  "manifest_version": 3,
  "name": "LinkedIn Job Data Enhanced",
  "version": "1.1",
  "permissions": [
    "storage",
    "scripting",
    "cookies",
    "tabs",
    "activeTab",
    "webRequest"
  ],
  "host_permissions": [
    "https://www.linkedin.com/*",
    "https://api-v3-test.jobslist.live/*",
    "https://www.jobslist.live/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"]
    }
  ]
}
